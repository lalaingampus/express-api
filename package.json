{
  "name": "finance-express-api",
  "version": "1.0.0",
  "description": "Finance tracking API (pemasukan, pengeluaran, hutang, rekap, nb) using Express + Sequelize + JWT",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:sync": "node -e \"require('./src/models').sequelize.sync({ alter: true }).then(() => console.log('Database synced'))\""
  },
  "keywords": [
    "express",
    "sequelize",
    "postgres",
    "jwt",
    "swagger",
    "finance"
  ],
  "author": "Audia",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
